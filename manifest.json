{
  "manifest_version": 2,
  "name": "Trigger Blocker",
  "description": "A Chrome Extension for censoring triggering content on the internet.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["jquery.min.js", "censor.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}
